<div class="container">
  <div class="row">
    <div class="col-md-offset-3 col-md-6">
      <h1 class="text-center text-white">Fivech</h1>
        
        <%= link_to 'スレッド新規登録', new_thread_path, class:'btn btn-success' %>
        
          <% @thre.each do |t|%>
            <div class="well">
              <%= link_to thread_path(id: t.id), method: :get do %>
                <%= t.title %>
                <%= t.created_at %>
              <% end %>
            </div>
          <% end %>
            
        
      </div>
    </div>
  </div>